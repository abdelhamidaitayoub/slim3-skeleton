{
  "name": "rrr3d/php-mvc",
  "type": "project",
  "description": "Slim PHP Micro-framework.",
  "license": "MIT",
  "authors": [
    {
      "name": "Abdelhamid Ait-Ayoub",
      "email": "abdelhamidaitayoub@gmail.com"
    }
  ],
  "scripts": {
    "start": [
      "php -S localhost:8080 -t public public/index.php"
    ]
  },
  "require": {
    "filp/whoops": "^2.12",
    "illuminate/database": "^8.40",
    "php-di/php-di": "^6.3",
    "slim/psr7": "^1.3",
    "slim/slim": "3.*",
    "slim/twig-view": "2.*",
    "vlucas/phpdotenv": "^5.3"
  },
  "require-dev": {
    "symfony/var-dumper": "^5.2"
  },
  "config": {
    "sort-packages": true,
    "process-timeout": 0,
    "platform": {
      "php": "7.4.0"
    }
  },
  "autoload": {
    "psr-4": {
      "Controller\\": "controller/",
      "Model\\": "model/",
      "Routes\\": "routes/",
      "View\\": "view/"
    }
  }
}
